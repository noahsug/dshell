[user]
	name = sugarman
	email = sugarman@google.com
[alias]
        rbm = rebase master
	rb = rebase
	cm = commit
	ca = commit -am
	co = checkout
	br = branch
	cp = cherry-pick
	di = diff
	st = status
        s = stash
        a = stash apply
	ft = fetch
	pl = pull
        lg = log
        sh = show
	tree = log --oneline --decorate --graph
        qlg = log --oneline -15
        qdi = diff --name-only
        qsh = show --name-only
[color]
	ui = auto
[core]
	autocrlf = false
	filemode = false
	editor = emacs -nw
[merge]
	tool = kdiff3
[push]
	default = simple
